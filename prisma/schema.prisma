datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Url {
  id        Int      @id @default(autoincrement())
  longUrl   String   @unique
  shortUrl  String   @unique
  createdAt DateTime @default(now())

  @@index([longUrl])
}
